{{#app-canvas}}
    <div class="inner-wrap">
        <!-- OFF CANVAS MENU BAR -->
        {{ top-navigation title="Account Balances" leftWidget="controls/side-menu-trigger"  }}
        <!-- END NAV.TAB-BAR HIDE-FOR-LARGE-UP -->

        {{side-menu}}

        <section class="main-section">
            <div class="row">
                <div class="small-12 columns">

                    <div class="block-card row">
                        <div data-equalizer>
                            <div class="block-card-details small-12 columns" data-equalizer-watch>
                                <h6 class="small subheader text-uppercase">2015</h6>
                                <h6 class="text-uppercase">FSA</h6>
                                <div class="row">
                                    <div class="small-9 columns end">
                                        <div class="time range-slider" data-slider data-options="initial: 80;">
                                            <span class="range-slider-handle" role="slider" tabindex="0"></span>
                                            <span class="range-slider-active-segment"></span>
                                            <input type="hidden">
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="small-9 columns">
                                        <div class="value range-slider" data-slider  data-options="initial: 60;">
                                            <span class="range-slider-handle" role="slider" tabindex="0"></span>
                                            <span class="range-slider-active-segment"></span>
                                            <input type="hidden">
                                        </div>
                                    </div>
                                    <div class="small-3 columns text-center">
                                        <span>$678.99</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="block-card row">
                        <div data-equalizer>
                            <div class="block-card-details small-12 columns" data-equalizer-watch>
                                <h6 class="small subheader text-uppercase">2014</h6>
                                <h6 class="text-uppercase">FSA</h6>
                                <div class="row">
                                    <div class="small-9 columns">
                                        <div class="value range-slider" data-slider  data-options="initial: 82;">
                                            <span class="range-slider-handle" role="slider" tabindex="0"></span>
                                            <span class="range-slider-active-segment"></span>
                                            <input type="hidden">
                                        </div>
                                    </div>
                                    <div class="small-3 columns text-center">
                                        <span>$338.95</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="block-card row">
                        <div data-equalizer>
                            <div class="block-card-details small-12 columns" data-equalizer-watch>
                                <h6 class="text-uppercase">Deductible</h6>
                                <div class="row">
                                    <div class="small-9 columns">
                                        <div class="value range-slider" data-slider  data-options="initial: 89;">
                                            <span class="range-slider-handle" role="slider" tabindex="0"></span>
                                            <span class="range-slider-active-segment"></span>
                                            <input type="hidden">
                                        </div>
                                    </div>
                                    <div class="small-3 columns text-center">
                                        <span>$3, 438.95</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </section>

        {{ bottom-navigation }}

        <!-- CLOSE THE OFF-CANVAS MENU -->
        <a class="exit-off-canvas"></a>
    </div>

    {{ modals/close-overview-tour action="disableOverviewTour" }}

{{/app-canvas}}